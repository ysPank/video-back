{
  "name": "video-backend",
  "engines": {
    "node": ">=12.0.0"
  },
  "version": "1.0.0",
  "author": "Yuriy Panko",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && babel src -d dist -s true",
    "eslint": "eslint -c ./.eslintrc.json src/**",
    "lint": "npm run eslint",
    "prestart": "npm run build",
    "start": "node dist/main.js",
    "watch": "npm run prebuild && babel src -d dist -s true --watch",
    "dev": "nodemon --exec babel-node ./src/main.js"
  },
  "dependencies": {
    "awilix": "4.2.7",
    "config": "3.3.2",
    "cors": "2.8.5",
    "express": "^4.16.4",
    "faker": "^5.5.3",
    "fast-json-stringify": "^1.9.1",
    "file-type": "10.11.0",
    "joi": "^14.0.2",
    "joi-to-json-schema": "^3.4.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "2.29.1",
    "moment-timezone": "0.5.31",
    "node-fetch": "2.6.1",
    "nodemailer": "^4.7.0",
    "passport": "0.4.1",
    "passport-http-bearer": "1.0.1",
    "redis": "^2.8.0",
    "socket.io": "^4.1.2",
    "swagger-ui-express": "^4.1.6",
    "twilio": "^3.65.0",
    "uuid": "8.3.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@types/express": "^4.16.0",
    "@types/node": "^8.10.36",
    "@types/redis": "^2.8.7",
    "@types/winston": "2.4.4",
    "babel-cli": "6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "eslint": "7.16.0",
    "husky": "^1.1.3",
    "nodemon": "^2.0.12",
    "rimraf": "3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
